{
  "name": "nativescript-cardview",
  "version": "2.0.0-rc.0",
  "description": "A NativeScript plugin for Material Design CardView component.",
  "main": "cardview",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "2.3.0",
      "ios": "2.3.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo; cd demo; tns emulate ios",
    "demo.android": "npm run preparedemo; cd demo; tns emulate android",
    "preparedemo": "npm run build; cd demo; tns plugin remove nativescript-cardview; tns plugin add ..; tns install",
    "setup": "cd demo; npm install; cd ..; npm run build; cd demo; tns plugin add ..; cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bradmartin/nativescript-cardview"
  },
  "keywords": [
    "NativeScript",
    "native",
    "script",
    "TypeScript",
    "javascript",
    "Card",
    "android",
    "ios",
    "CardView",
    "Material Design",
    "bradmartin",
    "Thorum"
  ],
